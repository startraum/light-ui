{
  "name": "light-ui",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "docker-build-amd64": "docker build -t startraum/light-ui:amd64 . && docker tag startraum/light-ui:amd64 startraum/light-ui:latest",
    "docker-build-arm": "docker build -f Dockerfile.arm -t startraum/light-ui:arm .",
    "docker-build": "npm run docker-build-amd64 && npm run docker-build-arm",
    "docker-push-amd64": "docker push startraum/light-ui:amd64",
    "docker-push-arm": "docker push startraum/light-ui:arm",
    "docker-push": "npm run docker-push-amd64 && npm run docker-push-arm",
    "release-amd64": "npm run docker-build-amd64 && npm run docker-push-amd64",
    "release-arm": "npm run docker-build-arm && npm run docker-push-arm",
    "release": "npm run release-amd64 && npm run release-arm",
    "start": "NODE_ENV=production node .next/production-server/server/index.js",
    "postinstall": "npm run build",
    "lint": "tslint --project .",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@types/bezier-easing": "^2.0.0",
    "@types/color": "^3.0.0",
    "@types/cookie": "^0.3.2",
    "@types/express": "^4.16.1",
    "@types/immutability-helper": "^2.6.3",
    "@types/jest": "23.3.12",
    "@types/next": "^7.0.6",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-ga": "^2.3.0",
    "@types/redis": "^2.8.12",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.6",
    "babel-eslint": "^9.0.0",
    "nodemon": "^1.18.9",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@radial-color-picker/rotator": "^2.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "bezier-easing": "^2.1.0",
    "color": "^3.1.0",
    "cookie": "^0.3.1",
    "express": "^4.16.4",
    "immutability-helper": "^2.9.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.7",
    "react-input-range": "^1.3.0",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.1.3"
  }
}
